  "scripts": {
    "docker": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml down up -d --build -V",
    "docker:down": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml down",
    "docker:app:log": "docker logs -f nestjs-example-app_app-server_1",
    "docker:prune": "docker system prune",
    "nest:generate": "nest generate resource",
    "prisma:init": "npx prisma init --datasource-provider sqlite",
    "prisma:generate": "npx prisma generate",
    "prisma:reset": "npx prisma migrate reset",
    "prisma:dev": "npx prisma migrate dev",
    "prisma:deploy": "npx prisma migrate deploy",
    "prisma:push": "npx prisma db push",
    "prisma:pull": "npx prisma db pull",
    "prisma:studio": "npx prisma studio",
    "husky:init": "npx husky install",
    "dev": "nest start --watch",
    "build": "nest build",
    "prod": "node dist/main",
    "debug": "nest start --debug --watch",
    "prebuild": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --watch --no-cashe --config ./test/jest-e2e.json",
    "lint:init": "eslint --init",
    "prepare": "husky install",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prettier:fix": "prettier --config .prettierrc.cjs \"src/**/*.{js,ts}\" --write"
  },
